<!--pages/welcome/welcome.wxml-->
<view class="login-all">
  <image class="logo" src="{{logo}}"></image>
  <view class="name-group">
    <image src="{{userlogin}}"></image>
    <input class="name" placeholder-class="name-input" placeholder="请输入账户" bindinput="getUname" value="{{keyValue}}"></input> 
    <image src="{{qc}}" catchtap="onClearTap"></image>
  </view>
  <view class="word-group">
    <image src="{{passlogin}}"></image>
    <input class="word" placeholder-class="word-input" placeholder="请输入密码" bindinput="getUword" password="{{isShow}}"></input>
    <image src="{{xs}}" catchtap="onShowTap" class="xs"></image>
  </view>
  <view catchtap="onInTap" class="journey-container">
    <text class="journey">{{title1}}</text>
  </view>
  <view class="last">
    <text class="last-container1">{{title2}}</text>
    <view catchtap="onUpTap">
      <text class="last-container2">{{title3}}</text>
    </view>
    <text class="last-container1">{{title4}}</text>
    <view catchtap="loginTap">
      <text class="last-container2">{{title5}}</text>
    </view>
  </view>
</view>